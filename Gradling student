#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
int n,i,r=0,v=0;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    for(i=0;i<n;i++)
    {   
        r=a[i]%5;
        if(r>=3&&a[i]>37)
        {
            v= a[i]-r+5;
        printf("%d\n",v);
        }
        else
            printf("%d\n",a[i]);
    }
        




}
